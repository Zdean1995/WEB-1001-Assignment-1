<!--Refences: https://www.w3schools.com/tags/att_output_form.asp-->
<form id="numform" oninput="x.value=parseInt(a.value)+parseInt(b.value)">
    <input type="number" id="a" name="a" value="0"><br />
    <input type="number" id="b" name="b" value="0"><br />
    <output form="numform" id="x" name="x" for="a+b">0</output><br />
    <input type="submit">
</form>

